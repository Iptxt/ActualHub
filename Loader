local TextChatService = game:GetService("TextChatService")
local TextChannels = TextChatService:WaitForChild("TextChannels"):WaitForChild("RBXGeneral")

TextChannels:DisplaySystemMessage("<font color='#FAF206'>[Actual Hub] </font>Script failed to load? try change <font color='#FAF206'>executor</font>.")

local GameList = {
    {
        placeId = { 3851622790, 2832883232 },
        Url = "https://raw.githubusercontent.com/Iptxt/ActualHub/refs/heads/main/BreakIn1"
    },
    {
        placeId = { 13864661000, 13864667823 },
        Url = "https://raw.githubusercontent.com/Iptxt/ActualHub/refs/heads/main/BreakIn2",
    },
    {
        placeId = { 126509999114328 },
        Url = "https://raw.githubusercontent.com/Iptxt/ActualHub/refs/heads/main/99NightsInTheForest",
    },
    {
        placeId = { 118614517739521 },
        Url = "https://raw.githubusercontent.com/Iptxt/ActualHub/refs/heads/main/Blindshot",
    },
}

-- \\ Selecting Game // --
local selectedUrl

for _, list in ipairs(GameList) do
    if table.find(list.placeId, game.PlaceId) then
        selectedUrl = list.Url
        break
    end
end

if selectedUrl then
    loadstring(game:HttpGet(selectedUrl))()
else
    TextChannels:DisplaySystemMessage("<font color='#FAF206'>[Actual Hub] </font>Game Is Not Supported! Executing <font color='#FAF206'>Alternative</font> script..")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Iptxt/AXHub-Loader/refs/heads/main/Alternative"))()
end










-- \\ Discord Link // --
task.defer(function()
    task.wait(2)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Iptxt/AXHub-Loader/refs/heads/main/DCLINK"))();
end)
